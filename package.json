{
  "name": "hakerbay-backend",
  "version": "1.0.0",
  "description": "hacker bay developer backend task",
  "main": "server.js",
  "scripts": {
    "test": "nyc --require @babel/register mocha src/__test__/**.spec.js --timeout 20000 --exit",
    "start": "node build/server.js",
    "start:dev": "DEBUG=development nodemon --exec babel-node src/server.js",
    "build": "babel src --copy-files --out-dir dist"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run prettier",
      "git add .",
      "npm test"
    ]
  },
  "author": "Timi Tejumola",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@hapi/joi": "^16.1.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "husky": "^3.0.5",
    "jsonpatch": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^9.3.0",
    "mocha": "^10.2.0",
    "multer": "^1.4.2",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "parser": "^0.1.4",
    "sharp": "^0.23.0",
    "swagger-ui-express": "^4.1.1",
    "uuidv4": "^5.0.1"
  },
  "devDependencies": {
    "debug": "^4.1.1"
  }
}
